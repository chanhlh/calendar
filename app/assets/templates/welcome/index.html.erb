 <div class="wrap-calendar">
  <br>
  <form class="form-horizontal" novalidate="" ng-submit="addEvent()">
    <div class="form-group">
      <label for="inputEmail3" class="col-sm-2 control-label">Title</label>
      <div class="col-sm-10">
        <input type="text" class="form-control" ng-model="eventData.title">
      </div>
    </div>
    <div class="form-group">
      <label for="inputEmail3" class="col-sm-2 control-label">Start Date</label>
      <div class="col-sm-10">
        <div date-picker="" ng-model="eventData.start" > </div>
      </div>
    </div>
    <div class="form-group">
      <label for="inputEmail3" class="col-sm-2 control-label">End Date</label>
      <div class="col-sm-10">
        <div date-picker="" ng-model="eventData.end" > </div>
      </div>
    </div>
    <div class="form-group">
      <div class="col-sm-offset-2 col-sm-10">
        <button type="submit" class="btn btn-default">Add Event</button>
      </div>
    </div>
  </form>
   <br>
   <div calendar="" pos-right="posRight" pos-left="posLeft" pos-center="posCenter" default-date="'2015-02-12'" ng-data="eventDataList"></div>
 </div>
